<div id="<%= dom_id article %>">
  <%#= cl_image_tag @article.photo.key, height: 300, width: 400, crop: :fill %>
  <% @article.photos.each do |photo| %>
    <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>

  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>

</div>
